{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["../js/mlj"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./",
        "recurse": true,
        "access": "all"
    }
}